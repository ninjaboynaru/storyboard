{
	"extends": [
		"eslint-config-react",
		"eslint-config-prettier"
	],
	
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"mocha": true
	},
	
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {"jsx": true}
	},
	
	"rules": {
		 "new-cap": ["error", { "newIsCap": false, "capIsNew": false }],
		 "react/sort-comp": [0],
		 "react/prop-types": 0,
		 "eqeqeq": 0,
		 "prefer-reflect": 0,
		 "camelcase": 0,
		 "consistent-this": 0
	}



}